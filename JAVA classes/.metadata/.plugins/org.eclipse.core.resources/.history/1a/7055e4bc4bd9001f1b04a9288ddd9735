package com.programming.class1;

import java.util.*;
import java.util.ArrayList;

public class Iterator {

	public static void main(String[] args) {
		
		List<String> list1 = new ArrayList<>();
		
		list1.add("robin");
		list1.add("ted");
		list1.add("barney");
		list1.add("marvin");
		list1.add(null);
		list1.add(null);
		list1.add(null);
		list1.add("lily");
		list1.add("elizebeth");
		
		List<String> list2 = new ArrayList<>();
		
		list2.add("rachel");
		list2.add("Ross");
		list2.addAll(list1);
		
		System.out.println("Size of list is : "+list2.size());
		
		IteratorClass<String> itr = list2.iterator();
		
	}
	
}
